/ {
    /* Enhanced Mouse Input Listeners for Speed Control */
    &mmv_input_listener {
        // Mouse Slow Layer (Layer 3)
        LAYER_MouseSlow {
            layers = <3>;
            input-processors = <&zip_xy_scaler 1 9>;
        };
        // Mouse Fast Layer (Layer 4) 
        LAYER_MouseFast {
            layers = <4>;
            input-processors = <&zip_xy_scaler 3 1>;
        };
        // Mouse Warp Layer (Layer 5)
        LAYER_MouseWarp {
            layers = <5>;
            input-processors = <&zip_xy_scaler 12 1>;
        };
    };

    &msc_input_listener {
        // Mouse Slow Scroll
        LAYER_MouseSlow {
            layers = <3>;
            input-processors = <&zip_scroll_scaler 1 9>;
        };
        // Mouse Fast Scroll
        LAYER_MouseFast {
            layers = <4>;
            input-processors = <&zip_scroll_scaler 3 1>;
        };
        // Mouse Warp Scroll
        LAYER_MouseWarp {
            layers = <5>;
            input-processors = <&zip_scroll_scaler 12 1>;
        };
    };

    behaviors {
        /* Caps Lock behavior - tap for ESC, hold for right option */
        caps_esc: caps_esc {
            compatible = "zmk,behavior-hold-tap";
            label = "CAPS_ESC";
            #binding-cells = <2>;
            tapping-term-ms = <200>;
            quick-tap-ms = <150>;
            flavor = "tap-preferred";
            bindings = <&kp>, <&kp>;
        };

        /* Right Ctrl + Number layer access behaviors */
        rctrl_num_0: rctrl_num_0 {
            compatible = "zmk,behavior-mod-morph";
            label = "RCTRL_NUM_0";
            #binding-cells = <0>;
            bindings = <&kp N0>, <&to 0>;
            mods = <(MOD_RCTL)>;
        };

        rctrl_num_1: rctrl_num_1 {
            compatible = "zmk,behavior-mod-morph";
            label = "RCTRL_NUM_1";
            #binding-cells = <0>;
            bindings = <&kp N1>, <&to 1>;
            mods = <(MOD_RCTL)>;
