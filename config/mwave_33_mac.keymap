            quick-tap-ms = <300>;
            require-prior-idle-ms = <150>;
            flavor = "tap-preferred";
            bindings = <&kp>, <&kp>;
            hold-trigger-key-positions = <POS_6 POS_7 POS_8 POS_9 POS_0 POS_MINUS POS_EQUAL POS_BSPC POS_DEL 
                                         POS_Y POS_U POS_I POS_O POS_P POS_LBKT POS_RBKT POS_BSLH 
                                         POS_H POS_J POS_K POS_L POS_SEMI POS_SQT POS_RET POS_PGUP
                                         POS_N POS_M POS_COMMA POS_DOT POS_FSLH POS_RSHFT POS_UP POS_PGDN
