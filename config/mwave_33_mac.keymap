    /* Combos - removed problematic Fn combos */
    combos {
        compatible = "zmk,combos";
        
        /* Caps lock when pressing both shift keys */
        combo_caps {
            timeout-ms = <50>;
            key-positions = <POS_LSHFT POS_RSHFT>;  // Updated positions
            bindings = <&kp CAPS>;
            layers = <0 2>;
        };

        /* Meh modifier (Option + Ctrl + Shift) */
        combo_meh {
            timeout-ms = <50>;
            key-positions = <POS_LEFT_ALT POS_LCTRL>;  // Updated positions
            bindings = <&sk LA(LC(LSHFT))>;
            layers = <0 2>;
        };

        /* Hyper modifier (Cmd + Option + Ctrl + Shift) */
        combo_hyper {
            timeout-ms = <50>;
            key-positions = <POS_LEFT_GUI POS_LEFT_ALT>;  // Updated positions
            bindings = <&sk LG(LA(LC(LSHFT)))>;
            layers = <0 2>;
        };
    };

